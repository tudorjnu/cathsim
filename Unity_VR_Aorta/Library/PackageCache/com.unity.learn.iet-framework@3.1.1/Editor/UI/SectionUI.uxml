<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:VisualElement name="SectionUI" class="container--card">
        <ui:VisualElement name="Card" class="card">
            <ui:VisualElement name="ImageContainer" class="container--tutorial-icon">
                <ui:VisualElement name="TutorialImage" class="image--tutorial-icon">
                    <ui:VisualElement name="imgCheckmark" class="icon--tutorial-checkmark" />
                    <ui:VisualElement name="imgErrorCheckmark" class="icon--tutorial-error-checkmark" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="TextContainer" class="container--card-text">
                <ui:Label text="Editor UI" name="lblName" class="label--tutorial-name" />
                <ui:Label text="Make a simple change and playtest your game." name="lblDescription" class="label--tutorial-description" />
                <ui:Label text="COMPLETED" name="lblCompletionStatus" class="label--completed" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
