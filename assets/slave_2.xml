<mujoco>
  <compiler meshdir="meshes" angle="degree"/>
  <asset>
    <!-- main body -->
    <mesh file="slave/base.stl"/>
    <mesh file="slave/main_rail.stl"/>
    <mesh file="slave/support_back.stl"/>
    <mesh file="slave/support_front.stl"/>
    <mesh file="slave/support_front_nose.stl"/>
    <mesh file="slave/support_left.stl"/>
    <mesh file="slave/support_right.stl"/>
    <!-- slider_front -->
    <mesh file="slave/slider_front_body.stl"/>
    <mesh file="slave/slider_front_clamp_left.stl"/>
    <mesh file="slave/slider_front_clamp_right.stl"/>
    <!-- slider_middle -->
    <mesh file="slave/slider_middle_body.stl"/>
    <mesh file="slave/slider_middle_wheel_lower.stl"/>
    <mesh file="slave/slider_middle_wheel_upper.stl"/>
    <!-- slider_upper -->
    <mesh file="slave/rail_upper.stl"/>
    <mesh file="slave/rail_upper_support.stl"/>
    <mesh file="slave/slider_upper_body.stl"/>
    <mesh file="slave/slider_upper_clamp_left.stl"/>
    <mesh file="slave/slider_upper_clamp_right.stl"/>
    <!-- slider_rear -->
    <mesh file="slave/slider_rear_body.stl"/>
    <mesh file="slave/slider_rear_wheel_lower.stl"/>
    <mesh file="slave/slider_rear_wheel_upper.stl"/>
    <!-- cylinder -->
    <mesh file="slave/cylinder/cylinder.stl"/>
    <mesh file="slave/cylinder/cylinder_hull_1.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_2.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_3.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_4.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_5.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_6.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_7.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_8.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_9.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_10.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_11.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_12.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_13.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_14.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_15.stl"/>
		<mesh file="slave/cylinder/cylinder_hull_16.stl"/>

    <!-- colours -->
    <material name="dark_blue" rgba="0.21176470588235294 0.27450980392156865 0.3215686274509804 1"/>
    <material name="yellow" rgba="0.9686274509803922 0.9254901960784314 0.34901960784313724 1"/>
    <material name="orange" rgba="0.9137254901960784 0.30980392156862746 0.21568627450980393 1"/>
    <material name="red" rgba="0.4196078431372549 0.058823529411764705 0.10196078431372549 1"/>
    <material name="silver" rgba="0.6745098039215687 0.6745098039215687 0.6745098039215687 1"/>
  </asset>
  <default class="main">
    <default class="primary">
      <geom type="mesh" material="dark_blue" contype="0" conaffinity="0"/>
    </default>
    <default class="secondary">
      <geom type="mesh" material="yellow" contype="0" conaffinity="0"/>
    </default>
    <default class="slider">
      <joint axis="0 -1 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
      <motor ctrlrange="-1. 1." ctrllimited="true"/>
      <geom type="mesh" material="dark_blue" contype="0" conaffinity="0"/>
    </default>
    <default class="clamp">
      <joint type="slide" axis="0 -1 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
      <motor ctrlrange="-1. 1." ctrllimited="true"/>
      <geom type="mesh" material="red" contype="0" conaffinity="0"/>
    </default>
    <default class="wheel">
      <joint type="hinge" axis="-1 0 0" limited="false"/>
      <motor ctrlrange="-1. 1." ctrllimited="true"/>
      <geom type="mesh" material="silver" contype="0" conaffinity="0"/>
    </default>
  </default>
  <worldbody>
    <body name="slave" pos="0 0 0">
      <geom type="mesh" mesh="base" class="primary"/>
      <body name="main_rail_body">
        <geom type="mesh" mesh="support_back" class="primary"/>
        <geom type="mesh" mesh="support_front" class="primary"/>
        <geom type="mesh" mesh="support_left" class="secondary"/>
        <geom type="mesh" mesh="support_right" class="secondary"/>
        <geom type="mesh" mesh="support_front_nose" class="primary"/>
        <geom type="mesh" mesh="main_rail" class="secondary"/>
        <!-- slider_front -->
        <body name="slider_front_body">
          <joint name="slider_front" type="slide" axis="-1 0 0" range="0 0.2" limited="true" damping="1" armature="0"/>
          <geom type="mesh" mesh="slider_front_body" class="slider"/>
          <body>
            <joint name="slider_front_clamp_left" type="slide" axis="-1 0 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
            <geom type="mesh" mesh="slider_front_clamp_left" class="clamp"/>
          </body>
          <body>
            <joint name="slider_front_clamp_right" type="slide" axis="-1 0 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
            <geom type="mesh" mesh="slider_front_clamp_right" class="clamp"/>
          </body>
        </body>
        <!-- slider_middle -->
        <body name="slider_middle_body">
          <joint name="slider_middle" type="slide" axis="0 -1 0" range="0 0.2" limited="true" damping="1" armature="0"/>
          <geom type="mesh" mesh="slider_middle_body" class="slider"/>
          <body name="slider_middle_wheel_lower_body">
            <joint name="slider_middle_wheel_lower" pos="-0.00033 0.27355 0.059012" type="hinge" axis="0 1 0" limited="false"/>
            <geom type="mesh" mesh="slider_middle_wheel_lower" class="wheel"/>
          </body>
          <body name="slider_middle_wheel_upper_body">
            <joint name="slider_middle_wheel_upper" pos="-0.00033 0.27355  0.094901" type="hinge" axis="0 -1 0" limited="false"/>
            <geom type="mesh" mesh="slider_middle_wheel_upper" class="wheel"/>
          </body>
        </body>
        <!-- rail_upper -->
        <body name="rail_upper_body">
          <geom type="mesh" mesh="rail_upper" material="yellow" contype="0" conaffinity="0"/>
          <geom type="mesh" mesh="rail_upper_support" material="dark_blue" contype="0" conaffinity="0"/>

          <body name="slider_upper_body" pos="0.030038 0 0">
            <joint name="slider_upper" type="slide" axis="0 -1 0" range="-0.04 0" limited="true" damping="1" armature="0"/>
            <geom type="mesh" mesh="slider_upper_body" class="slider"/>
            <body>
              <joint name="slider_upper_clamp_left" type="slide" axis="-1 0 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
              <geom type="mesh" mesh="slider_upper_clamp_left" class="clamp"/>
            </body>
            <body>
              <joint name="slider_upper_clamp_right" type="slide" axis="-1 0 0" range="-0.002 0.002" limited="true" damping="1" armature="0"/>
              <geom type="mesh" mesh="slider_upper_clamp_right" class="clamp"/>
            </body>
          </body>
        </body>
        <!-- slider_rear -->
        <body name="slider_rear_body">
          <joint name="slider_rear" type="slide" axis="0 -1 0" range="0 0.2" limited="true" damping="1" armature="0"/>
          <geom type="mesh" mesh="slider_rear_body" class="slider"/>
          <body name="slider_rear_wheel_lower_body">
            <joint name="slider_rear_wheel_lower" pos="0.496956  -0.00033  0.059012" type="hinge" axis="0 1 0" limited="false"/>
            <geom type="mesh" mesh="slider_rear_wheel_lower" class="wheel"/>
          </body>
          <body name="slider_rear_wheel_upper_body">
            <joint name="slider_rear_wheel_upper" pos="0.496956  -0.00033  0.094901" type="hinge" axis="0 -1 0" limited="false"/>
            <geom type="mesh" mesh="slider_rear_wheel_upper" class="wheel"/>
          </body>
        </body>
      </body>
    </body>
    <body name="Cylinder">
      <geom type="mesh" mesh="cylinder" rgba=".99 .15 .15 1" group="0" contype="0" conaffinity="0"/>
      <geom type="mesh" mesh="cylinder_hull_1" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_2" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_3" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_4" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_5" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_6" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_7" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_8" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_9" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_10" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_11" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_12" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_13" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_14" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_15" rgba=".99 .15 .15 0" group="0"/>
      <geom type="mesh" mesh="cylinder_hull_16" rgba=".99 .15 .15 0" group="0"/>
    </body>
  </worldbody>
  <contact>
    <!-- <exclude body1="" body2=""/> -->
    <exclude body1="slider_front_body" body2="main_rail_body"/>
    <exclude body1="slider_front_body" body2="main_rail_body"/>
    <exclude body1="slider_middle_body" body2="main_rail_body"/>
    <exclude body1="slider_upper_body" body2="rail_upper_body"/>
    <exclude body1="slider_rear_body" body2="main_rail_body"/>

    <exclude body1="slider_middle_wheel_upper_body" body2="slider_middle_wheel_lower_body"/>
    <exclude body1="slider_rear_wheel_upper_body" body2="slider_rear_wheel_lower_body"/>
  </contact>
  <actuator>
    <!-- slider_front -->
    <motor name="slider_front_motor" joint="slider_front" class="slider"/>
    <motor name="slider_front_clamp_left_motor" joint="slider_front_clamp_left" class="clamp"/>
    <motor name="slider_front_clamp_right_motor" joint="slider_front_clamp_right" class="clamp"/>
    <!-- slider_middle -->
    <motor name="slider_middle_motor" joint="slider_middle" class="slider"/>
    <motor name="slider_middle_wheel_lower_motor" joint="slider_middle_wheel_lower" class="wheel"/>
    <motor name="slider_middle_wheel_upper_motor" joint="slider_middle_wheel_upper" class="wheel"/>
    <!-- slider_upper -->
    <motor name="slider_upper_motor" joint="slider_upper" class="slider"/>
    <motor name="slider_upper_clamp_left_motor" joint="slider_upper_clamp_left" class="clamp"/>
    <motor name="slider_upper_clamp_right_motor" joint="slider_upper_clamp_right" class="clamp"/>
    <!-- slider_rear -->
    <motor name="slider_rear_motor" joint="slider_rear" class="slider"/>
    <motor name="slider_rear_wheel_lower_motor" joint="slider_rear_wheel_lower" class="wheel"/>
    <motor name="slider_rear_wheel_upper_motor" joint="slider_rear_wheel_upper" class="wheel"/>
  </actuator>
</mujoco>
