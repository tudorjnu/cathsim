<mujoco model="scene">
	<size njmax="2000" nconmax="1000"/>
	<compiler meshdir="meshes" angle="degree"/>
	<option gravity="0 0 0" cone="elliptic" density="1000" viscosity="0.0009" solver="PGS" >
		<flag frictionloss="disable"/>
        </option>
	<asset >
		<texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="512" height="512"/>
		<texture name="texplane" type="2d" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="512" height="512" />
		<material name="matplane" reflectance="0" />

		<material name="blue" rgba="0.3882352941 0.4313725490 0.5019607843 1"/>
		<material name="silverish" rgba="0.7215686275 0.7137254902 0.6666666667 1"/>
		<material name="pale_gray" rgba="1 1 1 1"/>
		<!--[> <material name="silver" rgba="0.89 0.89 0.89 1" shininess="0.5" reflectance="0.2"/> <]-->
		<material name="black" rgba="0.15 0.15 0.15 1"/>
		<material name="dark_grey" rgba="0.16 0.17 0.18 1"/>
		<material name="lighter_dark_grey" rgba="0.25 0.25 0.25 1"/>
		<material name="grey" rgba="0.51 0.51 0.51 1"/>
		<material name="light_grey" rgba="0.7 0.7 0.7 1"/>
		<material name="cadet-blue-crayola" rgba="0.64 0.7 0.78 1"/>
		<material name="gainsboro"  rgba="0.87 0.87 0.89 1"/>
		<material name="white" rgba="0.99 0.99 0.99 1"/>
		<material name="baby-blue-eyes" rgba="0.58 0.75 0.92 1"/>
		<material name="alice-blue" rgba="0.88 0.9 0.93 1"/>
	</asset>
	<worldbody >
		<geom type="plane" size="1 1 1" rgba="1 1 1 0"/>
		<light directional="true" diffuse=".2 .2 .2" specular="0 0 0" pos="0 0 5" dir="0 0 -1" castshadow="false"/>
		<light directional="true" diffuse=".2 .2 .2" specular="0 0 0" pos="0 0 0" dir="0 0 1" castshadow="false"/>
		<light directional="false" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 4.0" dir="0 0 -1" castshadow="false"/>
		<camera name="top_camera" pos="0.007738 -0.069034 1.550" euler="0 0 0"/>
		<camera name="side_right_camera" pos="1.5 -0.35 1" quat="0.5 0.5 0.5 0.5"/>
		<camera name="side_left_camera" pos="-1.6 -0.35 1" quat="0.5 0.5 -0.5 -0.5"/>
		<camera name="profile_camera" pos="1.04 -1.18 1.61" quat="0.751 0.479 0.244 0.383"/>
		<!--<body name="camera" pos="0.0076103 -0.01 1.02" euler="0 0 90" >-->
		  <!--<joint name="camera_joint" pos="0 0 0" axis="0 0 -1" type="hinge" damping="0.04" />-->
		  <!--<geom type="sphere" size=".01" rgba="0 0 0 0"  contype="0" conaffinity="0" />-->
		  <!--<camera name="middle_camera" pos="0 -0.8 0" euler="90 0 0" />-->
		<!--</body>-->
	</worldbody>
	<actuator >
	<!--<motor name="slider_motor" joint="slider_joint" ctrlrange="-1 1" ctrllimited="true" forcerange="-0.1 0.1" forcelimited="true" />-->
	</actuator>
</mujoco>
